FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.8";
    object      Bz;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -2 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    Inlet
    {
        type            zeroGradient;
    }
    cilindro
    {
        type            zeroGradient;
    }
    front_back
    {
        type            empty;
    }
    outlet
    {
        type            zeroGradient;
    }
    simetria
    {
        type            zeroGradient;
    }
}


