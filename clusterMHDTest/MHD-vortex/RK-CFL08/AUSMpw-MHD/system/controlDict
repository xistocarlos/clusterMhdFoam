FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     clusterMHDFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         3.14;

deltaT         1e-8;

writeControl    adjustableRunTime;

writeInterval   3.14;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep yes;

maxCo 0.8;


